  action(type="omfile" file="<%= @log_root %>/ontask-web/ontask-web-stdout.log")
&stop

if (re_match($syslogtag,"^unisa-simple-queue:\$")) then
  action(type="omfile" file="<%= @log_root %>/simpleQueue/simpleQueue-stdout.log")
&stop

